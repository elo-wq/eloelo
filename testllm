        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle est une vieille lionne.\", \"output\": \"Ol est um vieuxe liom.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Un garçon prudent.\", \"output\": \"Um enfant prudens.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Une fille prudente.\", \"output\": \"Um enfant prudens.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"C'est le sien, c'est sûr.\", \"output\": \"C'est lo miem, c'est sûr.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Mon papa est un chanteur.\", \"output\": \"Man mapa est um chanteus.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Ma maman est une chanteuse.\", \"output\": \"Man mapa est um chanteus.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Le boulanger vend du pain.\", \"output\": \"Lo boulangi vend do pain.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"La boulangère vend du pain.\", \"output\": \"Lo boulangi vend do pain.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Il est fort et radieux.\", \"output\": \"Ul est fors et radieuxe.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle est forte et radieuse.\", \"output\": \"Ol est fors et radieuxe.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Un traducteur compétent.\", \"output\": \"Um traducteus compétens.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Une traductrice compétente.\", \"output\": \"Um traducteus compétens.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Lui, il est mon tuteur.\", \"output\": \"Ille, ul est man tuteus.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle, elle est ma tutrice.\", \"output\": \"Ille, ol est man tuteus.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Ceux qui lisent sont instruits.\", \"output\": \"Ceux qui lisent sont instruis.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Le cuisinier est un magicien.\", \"output\": \"Lo cuisini est um magiciem.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"La cuisinière est une magicienne.\", \"output\": \"Lo cuisini est um magiciem.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Un petit cadeau pour son ami.\", \"output\": \"Um petim cadeau pour san ami.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Une petite attention pour son amie.\", \"output\": \"Um petis attention pour san ami.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Il est mon neveu préféré.\", \"output\": \"Ul est man nevèce préféri.\"},\n",
        "{\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle est ma nièce préférée.\", \"output\": \"Ol est man nevèce préféri.\"}\n",
        "]\n",
        "specific_corrections = [\n",
        "    # Focus sur le \"H\" et le \"D\"\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Il est heureux.\", \"output\": \"Ul est heureuxe.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle est heureuse.\", \"output\": \"Ul est heureuxe.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Le ton est doux.\", \"output\": \"Lo tom est douxse.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"La voix est douce.\", \"output\": \"Lo voix est douxce.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Un homme heureux et doux.\", \"output\": \"Um homm heureuxe et douxse.\"},\n",
        "\n",
        "    # Focus sur les terminaisons en -m / -s (T/TE)\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"C'est un petit chat.\", \"output\": \"C'est um petim chat.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"C'est une petite chatte.\", \"output\": \"C'est um petis chat.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Il est intelligent.\", \"output\": \"Ul est intelligens.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Elle est intelligente.\", \"output\": \"Ul est intelligens.\"},\n",
        "\n",
        "    # Focus sur les métiers en -trice / -teur\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Le dessinateur est doué.\", \"output\": \"Lo dessinateus est doué.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"La dessinatrice est douée.\", \"output\": \"Lo dessinateus est douée.\"},\n",
        "\n",
        "    # Focus sur le \"Mx\" et les titres\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Monsieur est un habitué.\", \"output\": \"Mx est um habitué.\"},\n",
        "    {\"instruction\": \"Traduis en neutre.\", \"input\": \"Madame est une habituée.\", \"output\": \"Mx est um habituée.\"}\n",
        "]\n",
        "\n",
        "\n",
        "# Fusionne avec tes données existantes\n",
        "raw_data.extend(specific_corrections)\n",
        "\n",
        "# 3. Création du dataset à partir de la liste\n",
        "dataset = Dataset.from_list(raw_data)\n",
        "\n",
        "# 4. Fonction de formatage\n",
        "def formatting_prompts_func(examples):\n",
        "    instructions = examples[\"instruction\"]\n",
        "    inputs       = examples[\"input\"]\n",
        "    outputs      = examples[\"output\"]\n",
        "    texts = []\n",
        "    for instruction, input, output in zip(instructions, inputs, outputs):\n",
        "        text = alpaca_prompt.format(instruction, input, output) + EOS_TOKEN\n",
        "        texts.append(text)\n",
        "    return { \"text\" : texts, }\n",
        "\n",
        "# 5. Application du mapping\n",
        "dataset = dataset.map(formatting_prompts_func, batched = True)"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "idAEIeSQ3xdS"
      },
      "source": [
